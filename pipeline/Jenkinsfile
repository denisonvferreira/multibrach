pipeline {
    agent any

    stages {
        stage('Example') {
            steps {
                sh '''
                git rev-parse --abbrev-ref HEAD > GIT_BRANCH
                git_branch = readFile('GIT_BRANCH').trim()
                echo $(git_branch)
                echo ˜================================˜
                echo ˜$(env.GIT_BRANCH)˜
                echo ˜Teste leitura da variavel˜
                '''
            }
        }
    }
}
