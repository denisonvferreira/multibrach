pipeline {
    agent any
    stages {
        stage('Hello') {
               when {
                    anyOf { 
                        branch 'dev*'
                    }
               }
                steps {
                    echo "lendo branch  ${env.BRANCH_NAME}"
                    script {
                        sh '''
                         echo "Subindo imagem release na conta DEV"
                        '''
                    }
                }
               when {
                    anyOf { 
                        branch 'main*'
                    }
               }
               steps {
                    echo "lendo branch  ${env.BRANCH_NAME}"
                    script {
                        sh '''
                         echo "Subindo imagem release na conta main"
                        '''
                    }
               }            
        }
    }
}
