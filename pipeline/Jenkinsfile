pipeline {
    agent any
    stages {
        stage('Hello') {
               when {
                    anyOf { 
                        branch 'main*'
                        branch 'dev*'
                        branch 'rel/*'
                    }
               }
               steps {
                   script {
                    sh '''
                    echo "lendo branch  ${env.BRANCH_NAME}"
                    '''
                   }
               }
        }
    }
}
